{
  "name": "student-council-hosting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "db:create": "docker compose -f db.yml up -d",
